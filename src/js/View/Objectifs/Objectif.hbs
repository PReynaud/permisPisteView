<table class="table">
	<thead>
		<tr>
			<th>Identifiant</th>
			<th>Libellé</th>
			<th>Actions</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
			<tr>
				<td>{{objectif.numobjectif}}</td>
				<td>{{objectif.libobjectif}}</td>
				<td>
					{{#if actions}}
						<table class="table" id="tabAction">
							<thead>
								<tr>
									<th>Identifiant</th>
									<th>Libellé</th>
									<th></th>
								</tr>
							</thead>
							<tbody>
							{{#each actions}}
								<tr onClick="document.location='#Actions/{{attributes.numaction}}'">
									<td>{{attributes.numaction}}</td><td>{{attributes.libaction}}</td>
								</tr>
								{{/each}}
							</tbody>
						</table>
					{{/if}}
				<td>
				<td>
					<a href="#Objectifs/Modifier/{{objectif.numobjectif}}">
						<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
					</a>
					<a href="#Objectifs/Supprimer/{{objectif.numobjectif}}">
						<span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
					</a>
				</td>
			</tr>
	</tbody>
</table>