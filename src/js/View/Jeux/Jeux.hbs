<a href="#Jeux/Ajout">
<button type="button" class="btn btn-success">
  <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Ajouter Jeu
</button>
</a>
<table class="table">
	<thead>
		<tr>
			<th>Numéro du jeu</th>
			<th>Nom du jeu</th>
			<th>Mission<th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		{{#each jeux}}
			<tr onClick="document.location='#Jeux/{{numjeu}}'">
				<td>{{numjeu}}</td>
				<td>{{libellejeu}}</td>
				<td>numéro</td>
				<td>libellé</td>
				<td>
					<a href="#Jeux/Modifier/{{numjeu}}">
						<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
					</a>
					<a href="#Jeux/Supprimer/{{numjeu}}">
						<span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
					</a>
				</td>
			</tr>
			{{#each missionJeu}}
			<tr onClick="document.location='#Jeux/{{numjeu}}/Missions/{{nummission}}'">
				<td></td>
				<td></td>
				<td>{{nummission}}</td>
				<td>{{libmission}}</td>
				<td>
					<a href="#Jeux/{{numjeu}}/Missions/Modifier/{{nummission}}">
						<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
					</a>
					<a href="#Jeux/Missions/Supprimer/{{nummission}}">
						<span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
					</a>
				</td>
			</tr>
			{{/each}}
			<tr>
				<td></td>
				<td></td>
				<td>
				</a>
					<a href="#Jeux/{{numjeu}}/Missions/Ajout">
						<button type="button" class="btn btn-success">
						<span class="glyphicon glyphicon-plus" aria-hidden="true"></span>Ajouter Mission</button>
					</a>
				</td>
			</tr>
		{{/each}}
	</tbody>
</table>