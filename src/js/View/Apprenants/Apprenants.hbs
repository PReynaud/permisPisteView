<a href="#Apprenants/Ajout">
	<button type="button" class="btn btn-success">
	  <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Ajouter Apprenant
	</button>
</a>

<table class="table">
	<thead>
		<tr>
			<th>Identifiant</th>
			<th>Nom</th>
			<th>Prenom</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		{{#each apprenants}}
			<tr onClick="document.location='#Apprenants/{{attributes.numapprenant}}'">
				<td>{{attributes.numapprenant}}</td>
				<td>{{attributes.nomapprenant}}</td>
				<td>{{attributes.prenomapprenant}}</td>
				<td>
					<a href="#Apprenants/Modifier/{{attributes.numapprenant}}">
						<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
					</a>
					<a href="#Apprenants/Supprimer/{{attributes.numapprenant}}">
						<span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
					</a>
				</td>
			</tr>
		{{/each}}
	</tbody>
</table>