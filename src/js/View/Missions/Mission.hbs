<table class="table">
	<thead>
		<tr>
			<th>Identifiant</th>
			<th>Libellé Mission</th>
			<th>Objectifs lié</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
			<tr>
				<td>{{mission.nummission}}</td>
				<td>{{mission.libmission}}</td>
				<td>Identifiant</td>
				<td>Libellé Objectif</td>
				<td>
					<a href="#Jeux/{{mission.numjeu}}/Missions/Modifier/{{mission.nummission}}">
						<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
					</a>
					<a href="#Jeux/Missions/Supprimer/{{mission.nummission}}">
						<span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
					</a>
				</td>
			</tr>
			{{#each objectifs}}
			<tr onClick="document.location='#Objectifs/{{[2].numobjectif}}'">
				<td></td>
				<td></td>
				<td>{{[2].numobjectif}}</td>
				<td>{{[2].libobjectif}}</td>
				<td>
					<a href="#Objectifs/Modifier/{{[2].numobjectif}}">
						<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
					</a>
					<a href="#Objectifs/Supprimer/{{[2].numobjectif}}">
						<span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
					</a>
				</td>
			</tr>
			{{/each}}
			<tr>
				<td></td>
				<td></td>
				<td>
				</a>
					<a href="#Jeux/{{mission.numjeu}}/Missions/Modifier/{{mission.nummission}}/Objectifs/Ajout">
						<button type="button" class="btn btn-success">
						<span class="glyphicon glyphicon-plus" aria-hidden="true"></span>Fixer Objectif</button>
					</a>
				</td>
			</tr>
	</tbody>
</table>