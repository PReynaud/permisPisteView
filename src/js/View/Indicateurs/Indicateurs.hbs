<a href="#Indicateurs/Ajout">
<button type="button" class="btn btn-success">
		<span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Ajouter Indicateur
	
</button>
</a>
<table class="table">
	<thead>
		<tr>
			<th>Identifiant</th>
			<th>Action associée</th>
			<th>Libellé</th>
			<th>Poids</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		{{#each indicateurs}}
			<tr onClick="document.location='#Indicateurs/{{attributes.numindic}}'">
				<td>{{attributes.numindic}}</td>
				<td>{{attributes.numaction}}</td>
				<td>{{attributes.libindic}}</td>
				<td>{{attributes.poids}}</td>
				<td>
					<a href="#Indicateurs/Modifier/{{attributes.numindic}}">
						<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
					</a>
					<a href="#Indicateurs/Supprimer/{{attributes.numindic}}">
						<span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
					</a>
				</td>
			</tr>
		{{/each}}
	</tbody>
</table>